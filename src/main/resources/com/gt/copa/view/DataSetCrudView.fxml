<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="nodeView" prefHeight="354.0" prefWidth="562.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gt.copa.controller.DataSetCrudController">
   <items>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TableView fx:id="tblDataSets" editable="true" maxWidth="1.7976931348623157E308" prefWidth="1000.0">
              <columns>
                <TableColumn fx:id="colId" prefWidth="75.0" text="Id" />
                <TableColumn fx:id="colCodigo" prefWidth="75.0" text="Cod" />
                  <TableColumn fx:id="colNombre" prefWidth="300.0" text="Nombre" />
              </columns>
            </TableView>
            <VBox maxWidth="70.0" minWidth="70.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button alignment="CENTER" maxWidth="60.0" mnemonicParsing="false" onAction="#btnNuevoClick" text="Nuevo">
                     <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button alignment="CENTER" maxWidth="60.0" mnemonicParsing="false" onAction="#btnEliminarClick" text="Eliminar">
                     <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button alignment="CENTER" layoutX="15.0" layoutY="45.0" maxWidth="60.0" mnemonicParsing="false" onAction="#btnGuardarClick" text="Guardar">
                     <VBox.margin>
                        <Insets left="5.0" top="15.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children></HBox>
   </items>
</SplitPane>
